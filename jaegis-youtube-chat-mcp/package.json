    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",

    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "jaegis",
    "youtube",
    "chat",
    "cli",
    "mcp",
    "model-context-protocol",
    "ai",
    "video-analysis",
    "podcast",
    "transcription",
    "n8n",
    "rag",
    "tts",
    "text-to-speech",
    "channel-monitoring",
    "content-processing"
  ],
  "author": {
    "name": "JAEGIS Team",
    "email": "use.manus.ai@gmail.com",
    "url": "https://github.com/usemanusai/youtube-free-deep-research-cli"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/usemanusai/youtube-free-deep-research-cli.git",
    "directory": "jaegis-youtube-chat-mcp"
  },
  "bugs": {
    "url": "https://github.com/usemanusai/youtube-free-deep-research-cli/issues"
  },
  "homepage": "https://github.com/usemanusai/youtube-free-deep-research-cli#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "commander": "^12.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.4.0"
  },

  "files": [
    "dist/**/*",
    "bin/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "preferGlobal": true,
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
