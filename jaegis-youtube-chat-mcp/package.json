  "description": "JAEGIS YouTube Chat CLI - Model Context Protocol (MCP) Server for AI-powered YouTube video analysis and content processing",
  "main": "dist/index.js",
  "bin": {
    "jaegis-youtube-chat-mcp": "./bin/jaegis-youtube-chat-mcp"
  },
  "scripts": {
